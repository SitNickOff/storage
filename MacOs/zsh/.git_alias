unalias grs
alias grs='git reset --soft HEAD^1'

unalias gca
alias gca='git commit -v --no-edit --amend'

unalias gcam
alias gcam='git commit -v --amend -m'

unalias gc
alias gc='git commit -m '

alias gpl='git pull'
alias gplo='git pull origin $(git symbolic-ref --short HEAD)'

unalias gp
alias gp='git push'

unalias gpo
alias gpo='git push origin $(git symbolic-ref --short HEAD) --no-verify'

unalias gn
alias gpn='git push --no-verify'

unalias gpfn
alias gpfn='git push origin $(git symbolic-ref --short HEAD) --no-verify --force'

unalias gpnf
alias gpnf='git push origin $(git symbolic-ref --short HEAD) --no-verify --force'

unalias ga
alias ga='git add .'

unalias gs
alias gs='git status'

unalias gd
alias gd='git diff'

unalias gl
alias gl='git log'

unalias gf
alias gf='git fetch --all --prune'
